%h1 Admin Login

.well
  = form_tag '/login' do
    .control-group
    = label_tag :username, 'Username:'
    = text_field_tag :username
    %br
    %br
    .control-group
    = label_tag :password, 'Password:'
    = password_field_tag :password
    %br
    %br
    = submit_tag 'Login', class: 'btn btn-success'
