= form_for @category do |f|
  = render 'shared/errors', object: @category
  .form-horizontal
    .form-group
      =f.label :name, 'Category Name', class: 'control-label col-sm-2'
      .col-sm-2= f.text_field :name, class:'form-control'

  .col-sm-offset-2.col-sm-10= f.submit button, class: 'btn btn-primary'
